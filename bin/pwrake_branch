#!/usr/bin/env ruby

begin
  require 'rake'
rescue LoadError
  require 'rubygems'
  require 'rake'
end

libpath = File.absolute_path(File.dirname(__FILE__))+"/../lib"
$LOAD_PATH.unshift libpath
require "pwrake/master"
require "pwrake/branch"

Rake.application.run_branch($stdin,$stdout)

#system "ps vx 1>&2"
