#!/usr/bin/env ruby

begin
  require 'rake'
rescue LoadError
  require 'rubygems'
  require 'rake'
end

libpath = File.absolute_path(File.dirname(__FILE__))+"/../lib"
$LOAD_PATH.unshift libpath
require "pwrake/branch"

$stdout.puts "pwrake_branch started"
$stdout.flush

Rake.application.run

#system "ps vx 1>&2"
